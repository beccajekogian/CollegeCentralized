<%- include('../partials/header', {title:"College details"}) -%>


    <p>Add a college to your list<p>


  <form action="/opponents" method="POST">


    <label for="school">Pick the college you want to add to your list:</label>
    <select id="school" name="school">

      <%for(college in data){%>
      <option value="<%=data[college]%>"><%=data[college]%></option>
      <%}%>

    </select>

    <br><br>

    <input type="submit" value="Add School!">

  </form>

