<%- include('../partials/header', {title:"College details"}) -%>


    <h2>A <%=college%> supplement</h2>
  <br>

<dl>
      <dt>prompt: <%=supplement.prompt%></dt>
      <dt><%=supplement.wordMin%> to <%=supplement.wordMax%> words</dt>

</dl>

<form action="/students/<%=college%>/<%=supplement.supplementID%>" method="POST">
  <div class="form-group">
    <label for="content">Your writing: </label>
    <textarea class="form-control" id="content" name="content" rows="5" ><%=supplement.content%></textarea>
  </div>

  <input type="submit" value="save">
  <!-- <input type="submit" value="send to counselor"> -->

</form>


