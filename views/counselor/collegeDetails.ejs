<%- include('../partials/header', {title:"College details"}) -%>

<div id="accordion">

<% for (supplement in supplements){ %>

  <div class="card">
    <div class="card-header" id="headingOne">
      <h5 class="mb-0">
        <button class="btn btn-link" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
          <%=supplements[supplement]["prompt"]%>
        </button>
      </h5>
    </div>

    <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
      <div class="card-body">
        <h5 class="card-title"><%=supplements[supplement]["prompt"]%></h5>
        <h6 class="card-subtitle mb-2 text-muted"><%=supplements[supplement]["wordMin"]%>  to  <%=supplements[supplement]["wordMax"]%></h6>
        <p class="card-text"><%=supplements[supplement]["content"]%></p>
        <a href="/counselors/<%=student%>/<%=college%>/<%=supplements[supplement]["supplementID"]%>/edit" class="card-link"> link to supplement</a>

      </div>
    </div>
  </div>

<%}%>

</div>

<%- include('../partials/footer') -%>
