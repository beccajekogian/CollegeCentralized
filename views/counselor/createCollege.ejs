<%- include('../partials/header', {title:"College details"}) -%>


    <p>Create a college<p>


  <form action="/colleges/new" method="POST">
    <label for="collegeName">Name of college:</label>
    <input type="text" id="collegeName" name="collegeName" value=""><br>

    <input type="checkbox" name="Test" <%- check %> >

    <label for="applicationType1">Choose an application plan</label>
    <select id="applicationType" name="applicationType">
      <option value="ED">ED I</option>
      <option value="EDtwo">ED II</option>
      <option value="EA">Early Action</option>
      <option value="reg">regular</option>

    </select>
    <br><br>

    <input type="submit" value="Add School!">

  </form>

  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
